{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
        "eqeqeq": "warn",
        "indent": ["warn", 2, { "SwitchCase": 1 }],
        "multiline-ternary": ["warn", "always-multiline"]
      },
    "extends": [
      "eslint:recommended"
    ]
    
  }